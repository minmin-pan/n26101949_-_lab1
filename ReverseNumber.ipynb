    "def reverse(num):\n",
    "    \"\"\"\n",
    "    The function used to reverse your input number.\n",
    "    \"\"\"\n",
    "    re_num = ''\n",
    "    while num != 0: \n",
    "        re_num += str(num % 10)\n",
    "        num //= 10\n",
    "    for i in re_num:\n",
    "        print(i, end='')\n",
    "\n",
    "num = int(input('Enter an integer:'))\n",
    "reverse(num)"
